<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.econolodge.econolodgeapp3.FrontDeskFragment">


    <!-- TODO: Update blank fragment layout -->
    <RelativeLayout  android:layout_width="match_parent"
        android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        android:paddingBottom="@dimen/activity_vertical_margin"
        tools:context="com.econolodge.econolodgeapp3.FragmentContainerActivity$PlaceholderFragment"
        android:id="@+id/z"
        android:focusableInTouchMode="true"
       >
        <TableLayout
            android:id="@+id/tableLayout1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:shrinkColumns="*"
            android:stretchColumns="*"
            android:layout_gravity="fill"
            >
             <TableRow
                android:id="@+id/tableRow2"
                android:layout_height="wrap_content"
                android:layout_width="match_parent">
                <TextView
                    android:id="@+id/TextView1"
                    android:text="Room No."
                    android:layout_column="0"
                    android:typeface="serif"
                    android:textSize="13dp"
                    android:textStyle="bold"
                    android:textAllCaps="true"></TextView>
                <TextView
                    android:id="@+id/TextView2"
                    android:text="Roomtype"
                    android:textStyle="bold"
                    android:typeface="serif"
                    android:textSize="13sp"
                    android:textAllCaps="true"
                    android:layout_column="1"></TextView>
                <TextView
                    android:id="@+id/TextView3"
                    android:text="Available"
                    android:layout_column="0"
                    android:typeface="serif"
                    android:textSize="13dp"
                    android:textStyle="bold"
                    android:textAllCaps="true"></TextView>
                <TextView
                    android:id="@+id/TextView4"
                    android:text="Clean"
                    android:textStyle="bold"
                    android:typeface="serif"
                    android:textSize="13sp"
                    android:textAllCaps="true"
                    android:layout_column="1"></TextView>
            </TableRow>
            <ListView
                android:id="@+id/listview1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                tools:context="com.econolodge.econolodgeapp3.Rooms"
                android:transcriptMode="normal"

                android:fastScrollEnabled="false">
            </ListView>
        </TableLayout>
    </RelativeLayout>

</FrameLayout>
